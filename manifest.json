{
  "manifest_version": 2,
  "name": "Browse13",
  "version": "1.0",
  "author": "Jasu",

  "description": "Browse in ROT 13 - for times when HTTPS is not enough.",
  "icons": {
    "48": "icon_enabled.png"
  },

  "page_action": {
    "default_icon": "icon_disabled.png"
  },
  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent" : false
  }
}
